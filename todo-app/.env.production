# Production Environment Variables
# Copy this to .env and fill in your values

# Backend API URL (replace with your deployed backend URL)
VITE_API_URL=https://your-backend-url.onrender.com/api

# AI Provider API Keys (uses first available - priority: Groq > DeepSeek > Gemini)
# GROQ - FREE, fast, recommended!
VITE_GROQ_API_KEY=

# DeepSeek - Very cheap, good quality
VITE_DEEPSEEK_API_KEY=

# Google Gemini - Free tier available
VITE_GEMINI_API_KEY=

# App Configuration
VITE_APP_NAME=Student Study Planner
VITE_APP_VERSION=1.0.0
VITE_STORAGE_KEY=student-study-planner
VITE_ANALYTICS_CACHE_KEY=study-analytics-cache
